<%- partial('partials/nav.ejs', {location: 'addafterschoolactivity'}) %>

<div class="container">
    <h1>New After School Activity</h1>

    <div align="left">
        <form>
            <div class="form-group">
                <label for="add-rate-schedule-name">Name</label>
                <input type="text" class="form-control" id="add-rate-schedule-name" required>
            </div>
            <div class="form-group">
                <label for="add-rate-schedule-cost">Cost</label>
                <input type="text" class="form-control" id="add-rate-schedule-cost" required>
            </div>
            
           
            <button type="submit" class="btn btn-primary" onClick="createAfterSchoolActivity()">Submit</button>

        </form>
    </div>
</div>