<%- partial('partials/nav.ejs', {location: 'addstudent'}) %>

<div class="container">
    <h1>New Student</h1>

    <div align="left">
        <form>
            <div class="form-group">
                <label for="add-student-name">Name</label>
                <input type="text" class="form-control" id="add-student-name" required>
            </div>
            <div class="form-group">
                <label for="add-student-phone">Phone Number</label>
                <input type="text" class="form-control" id="add-student-phone" required>
            </div>
            <div class="form-group">
                <label for="add-student-rate-schedules">Rate Schedule</label>
                <select id="add-student-rate-schedules"></select>
            </div>
            <button type="submit" class="btn btn-primary" onClick="addStudent();">Submit</button>
        </form>
    </div>
</div>